<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.chainspower.mytab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.chainspower.itemdecoration.RecyclerDividerActivity">
        </activity>
        <activity android:name=".RecyclerViewActivity">
        </activity>
        <activity android:name="com.chainspower.navigation.NavigationActivity">
        </activity>
        <activity
            android:name=".LoginActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".DataBindingRecyclerViewActivity">
        </activity>
        <activity android:name=".DataBindingActivity">
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true">
        </service>

        <activity android:name=".DialogFragmentActivity">
        </activity>
        <activity android:name=".ConstrainActivity">
        </activity>
        <activity android:name=".MotionLayoutActivity">
        </activity>
        <activity
            android:name=".ArgbEvaluatorActivity"
            android:theme="@style/AppArgbTheme">
        </activity>
        <activity android:name=".KTXCoreStudyActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>